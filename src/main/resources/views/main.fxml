      </ScrollPane>
    </AnchorPane>
    <TabPane fx:id="mainTabPane" prefHeight="200.0" prefWidth="350.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="160.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="60.0" />
